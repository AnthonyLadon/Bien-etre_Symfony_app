{% extends 'base.html.twig' %}

{% block title %}
	Prestataire inscription
{% endblock %}

{% block titreh1 %}
  Inscription prestataire
{% endblock %}

{% block searchForm %}
	{% embed "_searchFormToggle.html.twig" %}{% endembed %}
{% endblock %}

{% block body %}

<h3 class="partenaire-registration-title">Vous n'êtes plus qu'à quelques clics de faire partie des prestataires de Bien-être!</h3>

  {{ form_start(form) }}

  <div class="form-register">
    <div class="form-register-input">
      {{ form_label(form.nom) }}
      {{ form_widget(form.nom) }}
    </div>
    <div class="form-register-input">
      {{ form_label(form.siteWeb) }}
      {{ form_widget(form.siteWeb) }}
    </div>
    <div class="form-register-input">
      {{ form_label(form.tel) }}
      {{ form_widget(form.tel) }}
    </div>
    <div class="form-register-input">
      {{ form_label(form.tvaNum) }}
      {{ form_widget(form.tvaNum) }}
    </div>
    <div class="form-register-input-multiple-select">
      {{ form_label(form.proposer) }}
      {{ form_widget(form.proposer) }}
    </div>
    {{ form_widget(form.soumettre,{'attr': {'class': 'register-form-submit'}}) }}

  </div>
  {{ form_end(form) }}

{% endblock %}
